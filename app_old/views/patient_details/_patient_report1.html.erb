<br/>
	
	<h3>Patient Report: <%= @start %> - <%= @end %></h3>

<table class='table table-striped table-bordered'>
  <tr>
      <th>SI.No</th>
      <th>Date</th>
      <th>No of Patients</th>
  </tr>
	<% i = 0 %>
<% @patients.each do |key, value| %>
  <tr>
    <td><%= i += 1 %></td>
    <td><%= key %></td>
    <td><%= value %></td> 
  </tr>
<% end %>
</table>
